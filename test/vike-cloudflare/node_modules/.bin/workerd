#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules/workerd/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules/workerd/node_modules:/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules/workerd/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules/workerd/node_modules:/home/rom/code/vike/node_modules/.pnpm/workerd@1.20250507.0/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../node_modules/.pnpm/workerd@1.20250507.0/node_modules/workerd/bin/workerd"   "$@"
exit $?
