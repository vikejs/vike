#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules/vite/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules/vite/node_modules:/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules/vite/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules/vite/node_modules:/home/rom/code/vike/node_modules/.pnpm/vite@6.3.5_@types+node@18.19.100_terser@5.38.1_tsx@4.19.4/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
