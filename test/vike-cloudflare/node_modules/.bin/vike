#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rom/code/vike/vike/node/cli/node_modules:/home/rom/code/vike/vike/node/node_modules:/home/rom/code/vike/vike/node_modules:/home/rom/code/vike/node_modules:/home/rom/code/node_modules:/home/rom/node_modules:/home/node_modules:/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rom/code/vike/vike/node/cli/node_modules:/home/rom/code/vike/vike/node/node_modules:/home/rom/code/vike/vike/node_modules:/home/rom/code/vike/node_modules:/home/rom/code/node_modules:/home/rom/node_modules:/home/node_modules:/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vike/node/cli/bin.js" "$@"
else
  exec node  "$basedir/../vike/node/cli/bin.js" "$@"
fi
