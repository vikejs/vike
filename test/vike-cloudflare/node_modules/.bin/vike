#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/richardunterberg/Documents/work26/vike/docs/vike/packages/vike/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/packages/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/node_modules:/Users/richardunterberg/Documents/work26/vike/node_modules:/Users/richardunterberg/Documents/work26/node_modules:/Users/richardunterberg/Documents/node_modules:/Users/richardunterberg/node_modules:/Users/node_modules:/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/richardunterberg/Documents/work26/vike/docs/vike/packages/vike/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/packages/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/node_modules:/Users/richardunterberg/Documents/work26/vike/node_modules:/Users/richardunterberg/Documents/work26/node_modules:/Users/richardunterberg/Documents/node_modules:/Users/richardunterberg/node_modules:/Users/node_modules:/node_modules:/Users/richardunterberg/Documents/work26/vike/docs/vike/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vike/bin.js" "$@"
else
  exec node  "$basedir/../vike/bin.js" "$@"
fi
