#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules/terser/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules/terser/node_modules:/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules/terser/bin/node_modules:/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules/terser/node_modules:/home/rom/code/vike/node_modules/.pnpm/terser@5.38.1/node_modules:/home/rom/code/vike/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/terser@5.38.1/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/terser@5.38.1/node_modules/terser/bin/terser" "$@"
fi
