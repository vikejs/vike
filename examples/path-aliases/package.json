{
  "imports": {
    "#root/*": "./*.ts"
  },
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "prod:static": "npm run build && npm run build:prerender && npm run server:static",
    "build": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "server:dev": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "server:static": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "serve": "^13.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.7",
    "vite-plugin-ssr": "0.3.64"
  }
}
