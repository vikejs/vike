{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_date": "2025-08-06",
  "name": "vike_examples_cloudflare-workers-react",
  "main": "./worker/index.js",
  "dev": { "port": 3000 },
  "define": {
    // Workaround https://github.com/cloudflare/workers-sdk/issues/7886
