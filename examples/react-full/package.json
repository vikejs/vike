{
  "scripts": {
    "start": "npm run dev",
    "dev": "node ./server",
    "prod": "npm run build && npm run prod:serve",
    "// Build for production": "",
    "build": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "build:clean": "rimraf dist/",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "// Static serve for production": "",
    "prod:serve": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "@mdx-js/react": "^2.0.0",
    "@mdx-js/rollup": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.19",
    "@types/node-fetch": "^2.5.12",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-fetch": "^3.1.5",
    "express": "^4.17.3",
    "node-fetch": "^2.6.1",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-ssr": "0.3.60"
  },
  "type": "module"
}
